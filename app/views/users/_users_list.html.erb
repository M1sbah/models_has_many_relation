<table class="table mt-5" id="users-list">
    <thead class="table table-primary text-center">
        <th>Name</th>
        <th>Address</th>
        <th>Actions</th>
    </thead>
    <tbody>
        <% users.each do |user| %>
            <%= render 'user_item', user: user %>
        <% end %>
    </tbody>

</table>